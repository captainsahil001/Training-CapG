package conditional_stmt;

public class Big4 {
	public static void main(String[] args) {
		int a=10,b=20,c=30,d=25;
		int big = (a>b && a>c)?a:(b>c)?b:c;
		if(big>d) {
			System.out.println(big + "is the biggest number");
		}
		if(d>big) {
			System.out.println(d + "is the biggest number");
		}
	}

}
