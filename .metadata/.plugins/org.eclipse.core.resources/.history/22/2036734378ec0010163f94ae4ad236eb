package Java_Core;

public class Demo {
	public static void main(String[] args) {
		int a = 10;
		long b = a; // widening
		System.out.println(b);

		long c = 20;
		int d = (int) c; // narrowing
		System.out.println(d);

		double dl = 55.55;
		int il = (int) dl; // narrowing
		System.out.println(il);
	}

}
