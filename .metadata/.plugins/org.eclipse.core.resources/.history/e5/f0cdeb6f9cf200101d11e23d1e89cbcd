package oops.polymorphism.runtime;

public class Child2 {
	public static void home2() {
		System.out.println("RED");
	}
	public static void bike2() {
		System.out.println("BMW");
	}
	public static void main(String[] args) {
		Parent p = new Parent();
		p.home();
		Child c =new Child();
		c.home();
		c.bike();
		Parent p2 = new Child();
		p2.home();
		}

}
