package method;

public class PrimeN {
	public static boolean isPrime(int n) {
		for (int i =2; i<=n/2;i++) {
			if(n %i==0) 
				return false;
		}
			
			return true;
		}
	public static void main(String[] args) {
		if(isPrime(20)) {
			System.out.println("Is a Prime Number");
		}
		else {
			System.out.println("Not a Prime Numbner");
		}
	}
	}


