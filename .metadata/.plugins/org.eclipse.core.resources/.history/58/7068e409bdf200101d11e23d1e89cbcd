package oops.array;

public class ReturnEvenArr {
	public static int[] findEvenElement(int[] arr) {
		int []a = new int[10];
		for(int i=0;i<arr.length;i++) {
			if(arr[i]%2==0) {
				a[i]=arr[i];
			}
		}
		for(int i=0;i<a.length;i++) {
			System.out.print(a[i] + " ");
		}
		System.out.println();
		return a;
	}
	public static void main(String[] args) {
		int[] a = {3,4,5,6,7,8};
		findEvenElement(a);
	}

}
