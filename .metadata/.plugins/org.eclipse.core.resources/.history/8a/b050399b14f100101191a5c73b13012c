package oops.encapsulation.copy;

public class Student {
	String name;
	private int pass;
	public int getPass() {
		return pass;
	}
	public void setpass(int pass) {
		this.pass=pass;
	}
	public static void main(String[] args) {
		Student s = new Student();
		s.name = "Raju";
		s.pass = 5555;
		s.setpass(s.pass);
		s.getPass();
		}

}
