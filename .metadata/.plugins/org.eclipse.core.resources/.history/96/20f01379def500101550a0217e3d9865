package oops.am;

public class Demo {
	String name;
	int id;
	Demo(int id, String name){
		this.id = id;
		this.name = name;
	}
//	@Override
	public String toString() {
		return "id:"+ id +" " + "name:" + name;
	}
	public static void main(String[] args) {
		Demo d = new Demo(10,"XYZ");
		System.out.println(d);
		System.out.println(d.toString());
	}

}
