package wrapper;

public class demo {
	public static void main(String[] args) {
		int n=10;
		Integer i=n;     //auto boxing
		int n2 = i;      //auto unboxing
		
		Integer i2=80;   //auto boxing
		
		
		Character c1='A';
		System.out.println(i2);
		System.out.println(n);
		System.out.println(c1);
	}
}
