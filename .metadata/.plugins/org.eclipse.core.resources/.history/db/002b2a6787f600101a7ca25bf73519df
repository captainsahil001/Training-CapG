package oops.string;

public class Demo {
	public static void main(String[] args) {
		
	
		
		String st = new String("xyz");
		String sc = new String("xyz");
		
		String a="xyz";
		String b="xyz";
		


		
		System.out.println(st.equals(sc));
		System.out.println(a.equals(b));
		
		
		System.out.println(st == sc);
		System.out.println(a == b);
		
	}

}
