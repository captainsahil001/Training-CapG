package loop;

public class PrimeNo {
	public static void main(String[] args) {
		int n = 17;
		if( n == 1) {
			System.out.println("Not Prime");
		}
		int count = 0;
		for(int i=2;i*i<=n;i++) {
			if(i%2==0) {
				count++;
			}
			if(count>0) {
				System.out.println("Prime");
			}
		}
		if(count == 0) {
			System.out.println("Not Prime");
		}
	}

}
