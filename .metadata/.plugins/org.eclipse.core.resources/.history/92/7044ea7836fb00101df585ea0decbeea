package Assignment;

public class Student {
	
	int id;
	String name;
	float marks;
	
	public Student() {
		
	}
	
	public Student(int id,String name,float marks) {
		this.id=id;
		this.name=name;
		this.marks=marks;
	}
	
	public String toString() {
		return "id: "+id+" name: "+name+" marks: "+marks;
	}

	
}
