package oops.encapsulation.copy;

public class Trainer {
	private int id;
	private String name;
	private long pass;
	public int getid() {
		return id;
	}
	public String getName() {
		return name;
	}
	public long getPass() {
		return pass;
	}
	public void setId(int id) {
		this.id = id;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setPass(long pass) {
		this.pass = pass;
	}
}
