package oops.polymorphism.runtime;

public class Child2 extends Parent2{
	public static void home2() {
		System.out.println("RED");
	}
	public static void bike2() {
		System.out.println("BMW");
	}
	public static void main(String[] args) {
		Parent p = new Parent();
		p.home2();
		Child c =new Child();
		c.home2();
		c.bike();
		Parent p2 = new Child();
		p2.home2();
		}

}
